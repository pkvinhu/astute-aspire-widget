<div class="form-container">
  <div class="wrapper">
<div id="form">
  <div class="form-section">
    <input type="text" name="search" class="city-input" placeholder="Search a city.." value=""/>
    <a class="not_submit" href="#" onclick="getStructuredContent()">Search</a>
    <a class="not_submit" onclick="window.location.reload();">Reset</a>
  </div>
  
<% if (data.names) { %>
<div class="form-section">
  <label for="name">Names</label>
  
    <ul class="list-names">
    <% data.names.forEach((n) => { %>
      <li><input class ="cb" type="checkbox" name="<%= n %>" value="<%= n %>"><%= n %></input></li>
    <% }); %>
  </ul>
  
</div>
<% } %>
<% if (data.values) { %>
<div class="form-section">
  <label for="values">Content Type</label>
  
    <ul class="list-content-type">
    <% data.values.forEach((v) => { %>
      <li><input class ="cb" type="checkbox" name="<%= v %>" value="<%= v %>"><%= v %></input></li>
    <% }); %>
  </ul>
  
</div>
<% } %>
<% if (data.facets) { %>
<div class="form-section">
  <label for="facets">Facets</label>
  
    <ul class="list-facets">
    <% data.facets.forEach((f) => { %>
      <li><input class ="cb" type="checkbox" name="<%= f %>" value="<%= f %>"></input></li>
    <% }); %>
  </ul>
  
</div>
<% } %>

</div>

<div class="custom-cards">
  <div class="custom-card-container">
    <div class="custom-label">Customize A Card</div>
    <div class="lp-json-pollock">
      <div class="lp-json-pollock-layout lp-json-pollock-layout-vertical">
        <div class="lp-json-pollock-element-image custom-img">
          <div class="custom-click" onclick="changeToInput('img')">Click to add image</div>
        </div>
        <div class="lp-json-pollock-element-button custom-title">
          <div class="custom-click" onclick="changeToInput('title')">Click to add title</div>
        </div>
        <div class="lp-json-pollock-element-button custom-desc">
          <div class="custom-click" onclick="changeToInput('desc')">Click to add description</div>
        </div>
        <div class="lp-json-pollock-element-button custom-link">
          <div class="custom-click" onclick="changeToInput('link')">Click to add address</div>
        </div>
      </div>
    </div>
  </div>
  <div class="custom-card-container-2">

  </div>
</div>

</div>
<div id="container">
  <div class="paging">
    <a href="#" class="page-left p" onclick="paging('left')">&laquo;</a>
    <div class="numbers p"></div>
    <a href="#" class="page-right p" onclick="paging('right')">&raquo;</a>
  </div>
  <div id="inner-container" data-value="1"></div>
</div>

</div>